{
  "compileOnSave": true,
  "compilerOptions": {
    "strict": true,
    "importHelpers": true,
    "noUnusedLocals": false,
    "noImplicitReturns": true,
    "removeComments": true,
    "declaration": true,
    "module": "System",
    "outDir": "./dist",
    "outFile": "./releases/jsb.js",
    "target": "es5",
    "baseUrl": "jsb-js",
    "lib": ["es2017"]
  },
  "include": ["./src/*.ts"]
}